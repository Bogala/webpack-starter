{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Initial project to use webpack for AngularJS",
  "main": "index.html",
  "scripts": {
    "test": "karma start karma.conf.js",
    "test-watch": "karma start karma.conf.js --auto-watch --no-single-run",
    "build": "webpack --bail --progress --profile -p",
    "dev": "webpack --bail --progress --profile -d --watch",
    "devserver": "webpack-dev-server --port 9100 --progress --colors --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bogala/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "angularjs"
  ],
  "author": "Benoit FONTAINE (benoit.a.fontaine@axa.fr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bogala/webpack-starter/issues"
  },
  "homepage": "https://github.com/Bogala/webpack-starter#readme",
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.0",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.12",
    "pug": "^2.0.0-beta6",
    "pug-html-loader": "^1.0.8",
    "pug-loader": "^1.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "@angular/router": "^0.1.0",
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.1.0",
    "angular-route": "^1.5.8"
  }
